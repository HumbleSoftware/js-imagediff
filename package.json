{
  "name": "imagediff",
  "main": "./imagediff.js",
  "browser": {
    "canvas": "./browser/canvas.js"
  },
  "description": "JavaScript Canvas based imagediff utility.",
  "homepage": "http://HumbleSoftware.github.com/js-imagediff/",
  "scripts": {
    "build": "smoosh make/build.json",
    "test": "karma start ./spec/karma.config.js --browsers PhantomJS --single-run; jasmine --config=./spec/jasmine.json",
    "watch": "karma start spec/karma.config.js --browsers PhantomJS"
  },
  "keywords": [
    "util",
    "server",
    "client",
    "browser",
    "canvas",
    "diff",
    "imagediff"
  ],
  "author": "Carl Sutherland <carl@humblesoftware.com> (http://www.humblesoftware.com/)",
  "contributors": [
    "Jordan Santell <@jsantell>"
  ],
  "optionalDependencies": {
    "canvas": ">=2.6.1"
  },
  "devDependencies": {
    "jasmine": "^3.5.0",
    "karma": "^4.4.1",
    "karma-jasmine": "^3.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "smoosh": ">=0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/HumbleSoftware/js-imagediff.git"
  },
  "version": "2.0.0-alpha.1",
  "engines": {
    "node": ">= 10.0.0"
  },
  "bin": {
    "imagediff": "./bin/imagediff"
  }
}
